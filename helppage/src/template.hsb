<!DOCTYPE html>
<HTML> 
	<meta charset="utf-8" />	
	<meta name=viewport content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no">
	<meta name=apple-mobile-web-app-capable content=yes>
	<meta name=mobile-web-app-capable content=yes>
	<!-- <meta http-equiv="Content-Type" content="text/html;charset=utf-8" /> -->
	<meta http-equiv=X-UA-Compatible content="IE=edge, chrome=1">	

    <meta name="libraryName" content="<%= htmlWebpackPlugin.options.libraryName %>" />
    <meta name="mdFileName" content="<%= htmlWebpackPlugin.options.mdFileName %>" />
    <meta name="headerMenuJson" content='<%= htmlWebpackPlugin.options.thisContext.headerMenu.json %>' />
    <meta name="menuJson" content='<%= htmlWebpackPlugin.options.thisContext.menu.json %>' />
    <meta name="menuOrderListJson" content='<%= htmlWebpackPlugin.options.thisContext.menuOrderList.json %>' />
    <meta name="saveJson" content='<%= htmlWebpackPlugin.options.thisContext.save.json %>' />



	<head>
		<title><%= htmlWebpackPlugin.options.title %></title>
		<!-- ★highlight.js -->
		<!--
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.7.0/styles/default.min.css">
            <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.7.0/highlight.min.js"></script>
		-->
		<link rel="stylesheet" href="../app/highlightjs/default.min.css">
        <script src="../app/highlightjs/highlight.min.js"></script>


        <!-- //////////////////////////////////////////////////
            ADSENSE 광고
        //////////////////////////////////////////////////-->
        <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
        <script>
             (adsbygoogle = window.adsbygoogle || []).push({
                  google_ad_client: "ca-pub-7259113564022979",
                  enable_page_level_ads: true
             });
        </script>


	</head>


                              
	<body class="boxman-1">


		<!-- TEST -->
		<!--
		<div data-box>test box</div>
		<div data-obj>test obj</div>
		<div data-obj>test obj</div>
		<div data-obj>test obj</div>
		-->


        <!-- /////////////////////////
             Header
        ///////////////////////// -->
        <div id="help-header" class="help-header test">
            <ul id="help-header-menulist" class="menu-type-a test">
            </ul>
        </div>


        <!-- /////////////////////////
            Context
        ///////////////////////// -->
		<div id="help-context" class="help-context">

			<!-- Menu -->
			<div id="help-menu" class="help-menu">
			    <div id="help-menu-context" style="width:100%; height:100%; background:#ffffff;">
                    <ul id="help-menu-menulist" class="menu-type-b test" >
                    </ul>
			    </div>
			</div>

			<!-- View -->
			<div id="help-view" class="help-view markdown github">
			    <%= htmlWebpackPlugin.options.thisContext.html %>
			</div>

		</div>


        <!-- /////////////////////////
            MINI-MENU
        ///////////////////////// -->
        <div id="mini-menu-button" style="position:fixed; top:50px; left:3px;">
            <button style="width:40px; height:40px; opacity:0.85; font-weight:bold;">...</button>
        </div>


        <!-- /////////////////////////
            Footer
        ///////////////////////// -->
        <div id="help-footer" class="help-footer">
        </div>

	</body>



</HTML>
